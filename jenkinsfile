pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                
                    "this is build"
                
                //
            }
        }
        stage('Test') {
            steps {
                
                    sh 'this is test'

                
                
                //
            }
        }
        stage('Deploy') {
            steps {
               sh 'echo this is deploy'
                
                //
            }
        }
    }

    post {
        always{
            echo "this sections run always"
        }
        sucess{
            echo "this section run when pipeline"
        }
    }
}